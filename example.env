# This value determines where the atproto files are pulled from. Gander developed or Bluesky developed.
BUILD_UPSTREAM_SOURCE=gander

# The env the app is running in e.g. development, testflight, production
EXPO_PUBLIC_ENV=development

# This is the semver release version of the app, pulled from package.json
EXPO_PUBLIC_RELEASE_VERSION=

# This is the commit hash that the current bundle was made from.
EXPO_PUBLIC_BUNDLE_IDENTIFIER=

# Should be formatted YYMMDDHH so that it increases for each build.
EXPO_PUBLIC_BUNDLE_DATE=0

# The log level for the app's logger transports
EXPO_PUBLIC_LOG_LEVEL=debug

# Enable debug logs for specific logger instances
EXPO_PUBLIC_LOG_DEBUG=session

# Chat service DID
EXPO_PUBLIC_CHAT_PROXY_DID=

# Apple services - update these once Apple developer account is set up
APPLE_APP_ID=XXXXXXXXXX
APPLE_BUNDLE_ID=xxx.xxx.xxx.xxx

# Primary default PDS service used for login (local PDS)
GNDR_SERVICE=http://localhost:2583

# Sentry DSN for telemetry
EXPO_PUBLIC_SENTRY_DSN=

# Bitdrift API key. If undefined, Bitdrift will be disabled.
EXPO_PUBLIC_BITDRIFT_API_KEY=

# -----------------------------------------------------------------------------
# AT Protocol core (from dev-env)
# PDS (auth + repos)
ATPROTO_PDS_URL=http://localhost:2583
# AppView (aggregated read API)
ATPROTO_APPVIEW_URL=http://localhost:2584

# API_URL feeds DEFAULT_SERVICE for auth flows â€“ must be PDS
API_URL=http://localhost:2583

# Canonical app/site URL (keep local unless you have a separate web front-end)
APP_URL=http://localhost:2584
SOCIAL_URL=http://localhost:2584
PROFILE_URL=http://localhost:2584/profile
DEEP_LINK_PREFIX=http://localhost:2584

# AppView specific
APPVIEW_URL=http://localhost:2584
PUBLIC_APPVIEW_URL=http://localhost:2584
DEV_ENV_APPVIEW_URL=http://localhost:2584

# Public AppView DIDs (use actual local AppView PLC DID)
PUBLIC_APPVIEW_DID=did:plc:4cxev3rron54pujcynxn43cw
PUBLIC_STAGING_APPVIEW_DID=did:plc:4cxev3rron54pujcynxn43cw

# Moderation / Ozone (if referenced)
MOD_SERVICE_URL=http://localhost:2587

# Optional auxiliary services (leave as prod defaults if not running locally)
# If you stand these up locally, point them to correct ports; remove incorrect prefixes
EMBED_SERVICE_URL=http://localhost:4001
OGCARD_SERVICE_URL=http://localhost:4002
GO_SERVICE_URL=http://localhost:4003
VIDEO_SERVICE_URL=http://localhost:4004
GIF_SERVICE_URL=http://localhost:4005

# Web policy / legal links (can keep remote until you host locally)
WEBLINK_TOS_URL=https://gndr.modcanada.ca/about/support/tos
WEBLINK_PRIVACY_URL=https://gndr.modcanada.ca/about/support/privacy-policy
WEBLINK_COMMUNITY_URL=https://gndr.modcanada.ca/about/support/community-guidelines
WEBLINK_COMMUNITY_DEPRECATED_URL=https://gndr.modcanada.ca/about/support/community-guidelines-deprecated