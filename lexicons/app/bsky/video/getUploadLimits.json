      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["canUpload"],
          "properties": {
            "canUpload": { "type": "boolean" },
            "remainingDailyVideos": { "type": "integer" },
            "remainingDailyBytes": { "type": "integer" },
            "message": { "type": "string" },
            "error": { "type": "string" }
          }
        }
      }
    }
  }
}
