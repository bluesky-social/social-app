{
  "lexicon": 1,
  "id": "chat.bsky.convo.acceptConvo",
  "defs": {
    "main": {
      "type": "procedure",
      "input": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["convoId"],
          "properties": {
            "convoId": { "type": "string" }
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "properties": {
            "rev": {
              "description": "Rev when the convo was accepted. If not present, the convo was already accepted.",
              "type": "string"
            }
          }
        }
      }
    }
  }
}
