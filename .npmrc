# Core settings for ATProto development
legacy-peer-deps=true
fund=false
audit=false
save-exact=false
package-lock=true
engine-strict=false

# Network and security settings
strict-ssl=false
registry=https://registry.npmjs.org/

# Protocol handling fixes for ESLint and link: errors
link=false
prefer-offline=true
no-optional=true
fetch-retries=3
fetch-retry-factor=10
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000

# Performance optimizations
progress=false
loglevel=warn
maxsockets=50

# Handle specific protocol errors
shrinkwrap=false
